{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "required": [
    "email",
    "password"
  ],
  "properties": {
    "email": {
      "type": "string",
      "minLength": 6,
      "maxLength": 60,
      "format": "email"
    },
    "password": {
      "type": "string",
      "minLength": 5,
      "maxLength": 20
    }
  },
  "additionalProperties": false
}

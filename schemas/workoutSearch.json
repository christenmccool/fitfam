{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "properties": {
    "swId": {
      "type": "string",
      "minLength": 1,
      "maxLength": 20
    },
    "name": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50
    },
    "description": {
      "type": "string",
      "minLength": 1,
      "maxLength": 500
    },
    "keyword": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50
    },
    "category": {
      "type": "string",
      "minLength": 1,
      "maxLength": 10
    },
    "scoreType": {
      "type": "string",
      "minLength": 1,
      "maxLength": 20
    },
    "publishDate": {
      "type": "string",
      "maxLength": 50,
      "format": "date"
    },
    
    "movementId": {
      "type": "array",
      "items":
        {
          "type": "string",
          "minLength": 1,
          "maxLength": 50
        }
     }
  },
  "additionalProperties": false
}

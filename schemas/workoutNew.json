{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "minProperties": 1,
  "properties": {
    "swId": {
      "type": "string",
      "minLength": 5,
      "maxLength": 20
    },
    "name": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50
    },
    "description": {
      "type": "string",
      "minLength": 1,
      "maxLength": 500
    },
    "scoreType": {
      "type": "string",
      "minLength": 1,
      "maxLength": 10
    },
    "publishDate": {
      "type": "string",
      "maxLength": 50,
      "format": "date"
    }
  },
  "additionalProperties": false
}

{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "required": [
    "userId", 
    "postId",
    "score"
  ],
  "properties": {
    "userId": {
      "type": "integer"
    },
    "postId": {
      "type": "integer"
    },
    "score": {
      "type": "object"
    },
    "notes": {
      "type": "string",
      "minLength": 1,
      "maxLength": 300
    },
    "completeDate": {
      "type": "string",
      "format": "date"
    }
  },
  "additionalProperties": false
}
